generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String  @unique
  email    String  @unique
  password String
  updated_at DateTime @updatedAt
   created_at DateTime @default(now()) 
 }
